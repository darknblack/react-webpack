{
  "name": "react-webpack-starter",
  "version": "1.0.0",
  "description": "React Webpack Starter Kit",
  "main": "index.js",
  "browserslist": [
    ">1%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-serve --config=config/webpack.config.js --open",
    "build": "cross-env NODE_ENV=production webpack --config=config/webpack.config.js",
    "build:prod": "cross-env NODE_ENV=production COMPRESSION=true webpack --config=config/webpack.config.js",
    "format": "prettier --write 'src/**/*.{js,jsx}'",
    "test": "jest --watchAll --coverage",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,css}": [
      "prettier --write 'src/**/*.{js,jsx}'",
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "automock": false,
    "testEnvironment": "node",
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/setupTests.js",
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ]
  },
  "repository": "https://github.com/darknblack/react-webpack-starter",
  "author": "Ian Ishmael Oderon",
  "license": "MIT",
  "engines": {
    "node": ">=8",
    "npm": ">=3"
  },
  "dependencies": {
    "bootstrap": "^4.1.3",
    "jquery": "^3.3.1",
    "lodash": "4.17.11",
    "popper": "^1.0.1",
    "prop-types": "15.6.2",
    "pug": "^2.0.3",
    "react": "16.5.1",
    "react-dom": "16.5.1",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "devDependencies": {
    "@babel/core": "7.0.1",
    "@babel/plugin-proposal-class-properties": "7.0.0",
    "@babel/plugin-proposal-decorators": "7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "7.0.0",
    "@babel/plugin-proposal-function-sent": "7.0.0",
    "@babel/plugin-proposal-json-strings": "7.0.0",
    "@babel/plugin-proposal-numeric-separator": "7.0.0",
    "@babel/plugin-proposal-throw-expressions": "7.0.0",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/plugin-syntax-import-meta": "7.0.0",
    "@babel/plugin-transform-react-constant-elements": "^7.0.0",
    "@babel/plugin-transform-react-inline-elements": "^7.0.0",
    "@babel/plugin-transform-runtime": "7.0.0",
    "@babel/polyfill": "7.0.0",
    "@babel/preset-env": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.0.0",
    "@babel/runtime": "7.0.0",
    "babel-core": "7.0.0-beta.3",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.2",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-transform-jsx-classname-components": "0.0.2",
    "babel-plugin-transform-react-pug": "^6.0.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.15",
    "brotli-webpack-plugin": "^1.0.0",
    "browser-sync": "^2.24.7",
    "browserslist": "4.1.1",
    "clean-webpack-plugin": "0.1.19",
    "compression-webpack-plugin": "^2.0.0",
    "cross-env": "5.2.0",
    "css-loader": "1.0.0",
    "enzyme": "3.6.0",
    "enzyme-adapter-react-16": "1.5.0",
    "eslint": "5.6.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.0.1",
    "eslint-loader": "2.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-react": "7.11.1",
    "eslint-plugin-react-pug": "^0.5.6",
    "eslint-watch": "4.0.2",
    "file-loader": "2.0.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "0.14.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "jest-enzyme": "6.0.4",
    "lint-staged": "7.2.2",
    "mini-css-extract-plugin": "0.4.2",
    "node-sass": "4.9.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "1.14.2",
    "pretty-quick": "1.6.0",
    "react-hot-loader": "^4.3.8",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.0",
    "uglifyjs-webpack-plugin": "^2.0.0",
    "webpack": "4.19.0",
    "webpack-cli": "3.1.0",
    "webpack-merge": "4.1.4",
    "webpack-serve": "2.0.2"
  }
}
